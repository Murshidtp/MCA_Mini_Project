 <div class="cont3">
      <h2 class="donatorh2">Donate Now</h2>
      <form action="/donatorhome" method="POST" autocomplete="on">
       <label class="donaterhomelabel">
        <span>Name</span>
        <input type="text" name="name">
      </label>
      <label class="donaterhomelabel">
        <span>Mobile No</span>
        <input type="tel" name="mobile">
      </label class="donaterhomelabel">
      <label>
        <span>Exact Location</span>
        <input type="text" name="location">
      </label>
      <label class="donaterhomelabel" for="district" name="district"><span>District</span></label>
      <label class="donaterhomelabel">
        <select class="otherslabel" id="category" name="district">
          <option value="" selected disabled hidden><span>Select District</span></option>
          <option value="Alappuzha"><span>Alappuzha</span></option>
          <option value="Ernamkulam"><span>Ernamkulam</span></option>
          <option value="Idukki"><span>Idukki</span></option>
          <option value="Kannur"><span>Kannur</span></option>
          <option value="Kasargod"><span>Kasargod</span></option>
          <option value="Kollam"><span>Kollam</span></option>
          <option value="Kottayam"><span>Kottayam</span></option>
          <option value="Kozhikkode"><span>Kozhikkode</span></option>
          <option value="Malappuram"><span>Malappuram</span></option>
          <option value="Palakkad"><span>Palakkad</span></option>
          <option value="Pathanamthitta"><span>Pathanamthitta</span></option>
          <option value="Thiruvanathapuram"><span>Thiruvananthapuram</span></option>
          <option value="Thrissur"><span>Thrissur</span></option>
          <option value="Wayanad"><span>Wayanad</span></option>
        </select>
      </label>
     <label class="donaterhomelabel">
        <span>Food Item</span>
        <input type="text" name="food">
      </label>
       <label class="donaterhomelabel">
        <span>Quantity</span>
        <input type="number" name="quantity">
      </label><center>
        <input type="submit" value="Submit" class="submit">

         
      </center>
      </form>
